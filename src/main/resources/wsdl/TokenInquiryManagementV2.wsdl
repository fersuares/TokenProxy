      <xsd:element name="PhoneExt" type="PhoneExt_Type"/>
      <xsd:simpleType name="PhoneExt_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:element name="CSPhoneNum" type="PhoneNumber_Type"/>
      <xsd:element name="ActivationPhone" type="PhoneNumber_Type"/>
      <xsd:element name="SafePhone" type="PhoneNumber_Type"/>
      <xsd:element name="Phone" type="PhoneNumber_Type"/>
      <xsd:element name="OrgPhone" type="PhoneNumber_Type"/>
      <xsd:element name="Fax" type="PhoneNumber_Type"/>
      <xsd:element name="EvePhone" type="PhoneNumber_Type"/>
      <xsd:element name="EveFax" type="PhoneNumber_Type"/>
      <xsd:simpleType name="PhoneNumber_Type">
		<xsd:annotation>
			<xsd:documentation>
				Tipo de dato para números telefónicos que extiende directamente de string de XSD.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
      <xsd:element name="DayPhone" type="PhoneNumber_Type"/>
      <xsd:element name="DayFax" type="PhoneNumber_Type"/>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza el país, el cual se basa en la norma ISO 3166, el tamaño 
	    	es de tres caracteres y aparece en las direcciones postales.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="Country" type="Country_Type"/>
      <xsd:simpleType name="Country_Type">
		<xsd:restriction base="NC">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
		<xsd:documentation>
			Este objeto se utiliza como contenedor general para los mensajes de error de la plataforma SOA. 
			Incluye internamente el agregado status de IFX.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:complexType name="GeneralException_Type">
		<xsd:sequence>
			<xsd:element minOccurs="1" ref="RqUID">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el identificador único del mensaje que entrega la aplicación 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="1" ref="Status">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la información del estado de la respuesta de la transacción.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:element name="GeneralException" type="GeneralException_Type"/>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza la información de referencia del objeto IFX.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="RefInfo" type="RefInfo_Type"/>
      <xsd:complexType name="RefInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="RefType" type="OpenEnum_Type">
				<xsd:annotation>
					<xsd:documentation>
						Tipo de referencia.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="RefId" type="Identifier_Type">
				<xsd:annotation>
					<xsd:documentation>
						Identificador de referencia.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="Name" type="Name_Type">
				<xsd:annotation>
					<xsd:documentation>
						Nombre de la referencia.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="Desc" type="Desc_Type">
				<xsd:annotation>
					<xsd:documentation>
						Descripción de la referencia.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza el encabezado de la petición del servicio.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:complexType name="SvcRq_Type">
		<xsd:sequence>
			<xsd:element minOccurs="1" ref="bbog:RqUID">
				<xsd:annotation>
					<xsd:documentation>
						Identificador único del mensaje que entrega la aplicación.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:AsyncRqUID">
				<xsd:annotation>
					<xsd:documentation>
						Identificador de solicitud asincrónica, enviado por un cliente para obtener una respuesta asíncrona 
						generada por un servidor, por lo general, en el caso de que la respuesta ha tomado demasiado tiempo 
						para construir y ser capaz de ser enviados de manera sincrónica.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:RevClientTrnSeq">
				<xsd:annotation>
					<xsd:documentation>
						Secuencia de transación del cliente para reversos.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:CustId">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la información del usuario que autentica el servicio.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:NextDay">
				<xsd:annotation>
					<xsd:documentation>
						El proveedor del pago indica que éste debe realizarse normalmente para el próximo día.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:BaseEnvr">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la información del ambiente que origina la invocación.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:ClientTerminalSeqId">
				<xsd:annotation>
					<xsd:documentation>
						 Identificador de secuencia generado por la terminal del cliente 
						 en un ambiente de oficina/CallCenter/cajero.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:TrnStatusType">
				<xsd:annotation>
					<xsd:documentation>
						Tipo de estado de la transacción.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:NetworkTrnInfo">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la información de la transacción en la Red sobre el procesamiento de la red, 
						es decir, propietario, ubicación, código de banco y el número de referencia asignado por la red durante 
						el procesamiento de la transacción.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="bbog:ClientDt">
				<xsd:annotation>
					<xsd:documentation>
						Tiempo de acuerdo con el cliente, se puede comparar con el tiempo del servidor
						para determinar si hay una discrepancia.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
		</xsd:sequence>
	</xsd:complexType>
      <xsd:element name="SvcRq" type="bbog:SvcRq_Type"/>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza el código de autorización o confirmación de una operación.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:simpleType name="AuthCode_Type">
    	<xsd:annotation>
        	<xsd:documentation>Codigo de autorizacion o confirmacion de una operacion.</xsd:documentation>
		</xsd:annotation>
    	<xsd:restriction base="C"/>
   	</xsd:simpleType>
      <xsd:element name="AuthCode" type="AuthCode_Type"/>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza la coordenada para determinar la posición de una dirección postal.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="Coordenate" type="Coordenate_Type"/>
      <xsd:simpleType name="Coordenate_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza la línea de dirección 4
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="Addr4" type="Addr4_Type"/>
      <xsd:simpleType name="Addr4_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
	    <xsd:documentation>			
			En este campo se parametriza la moneda de cuenta o divisa de la cuenta. 
			Esto puede ser necesario para identificar la cuenta, ya que muchos países 
			se permite para una sola cuenta para contener múltiples subcuentas, 
			cada uno en una moneda diferente.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:simpleType name="AcctCur_Type">
		<xsd:restriction base="bbog:NC">
			<xsd:maxLength value="22"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:element name="AcctCur" type="bbog:AcctCur_Type"/>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza el nombre del producto.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="AcctName" type="ext:AcctName_Type"/>
      <xsd:simpleType name="AcctName_Type">
		<xsd:restriction base="bbog:C">
			<xsd:maxLength value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza la línea de dirección 3
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="Addr3" type="Addr3_Type"/>
      <xsd:simpleType name="Addr3_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="64"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza la oficina próxima entrega o línea de dirección 2 
		</xsd:documentation>
	</xsd:annotation>
      <xsd:simpleType name="Addr2_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="64"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:element name="Addr2" type="Addr2_Type"/>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza la dirección de correo electrónica.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:simpleType name="EmailAddr_Type">
		<xsd:restriction base="bbog:NC">
			<xsd:maxLength value="128"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:element name="SafeEmailAddr" type="bbog:EmailAddr_Type"/>
      <xsd:element name="EmailAddr" type="bbog:EmailAddr_Type"/>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza la dirección postal.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="DeathInfoAddr" type="PostAddr_Type"/>
      <xsd:element name="BillRetAddr" type="PostAddr_Type"/>
      <xsd:element name="PostAddr" type="PostAddr_Type"/>
      <xsd:element name="RemitAddr" type="PostAddr_Type"/>
      <xsd:complexType name="PostAddr_Type">
		<xsd:sequence>
			<xsd:sequence>
				<xsd:element minOccurs="0" ref="Addr1">
					<xsd:annotation>
						<xsd:documentation>
							En este campo se parametriza la oficina de entrega o línea de dirección 1.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence minOccurs="0">
					<xsd:element minOccurs="0" ref="Addr2">
						<xsd:annotation>
							<xsd:documentation>
								En este campo se parametriza la oficina próxima entrega o línea de dirección 2.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:sequence minOccurs="0">
						<xsd:element minOccurs="0" ref="Addr3">
							<xsd:annotation>
								<xsd:documentation>
									En este campo se parametriza la línea de dirección 3.
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence minOccurs="0">
							<xsd:element minOccurs="0" ref="Addr4">
								<xsd:annotation>
									<xsd:documentation>
										En este campo se parametriza la línea de dirección 4.
									</xsd:documentation>
								</xsd:annotation>							
							</xsd:element>
						</xsd:sequence>
					</xsd:sequence>
				</xsd:sequence>
			</xsd:sequence>
			<xsd:element minOccurs="0" ref="City">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la ciudad dentro de la dirección postal.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="StateProv">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el código de provincia o estado.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="PostalCode">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el código postal.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Country">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el país, el cual se basa en la norma ISO 3166, el tamaño 
	    				es de tres caracteres y aparece en las direcciones postales.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AddrType">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el tipo de dirección física o electrónica.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="ZipCode">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el código de zip.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Coordenate">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la coordenada para determinar la posición de una dirección postal.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza la oficina de entrega o línea de dirección 1
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="Addr1" type="Addr1_Type"/>
      <xsd:simpleType name="Addr1_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza la ciudad dentro de la dirección postal. 
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="CityId" type="bbog:City_Type"/>
      <xsd:element name="City" type="bbog:City_Type"/>
      <xsd:simpleType name="City_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza el código de provincia o estado.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:element name="StateProv" type="bbog:StateProv_Type"/>
      <xsd:simpleType name="StateProv_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:annotation>
	    <xsd:documentation>
	    	En este campo se parametriza el código de zip.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:simpleType name="ZipCode_Type">
		<xsd:restriction base="C">
			<xsd:maxLength value="11"/>
		</xsd:restriction>
	</xsd:simpleType>
      <xsd:element name="ZipCode" type="ZipCode_Type"/>
      <xsd:annotation>
		<xsd:documentation>
			En este campo se parametriza la información de contacto.
		</xsd:documentation>
	</xsd:annotation>
      <xsd:complexType name="ContactInfo_Type">
		<xsd:sequence>
		    <xsd:element minOccurs="0" ref="ContactId">
		    	<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el identificador de contacto.
					</xsd:documentation>
				</xsd:annotation>
		    </xsd:element>
			<xsd:element minOccurs="0" ref="ContactPref">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el método preferido para llegar a la entidad a la que el se refiere.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="PhoneNum">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el número del teléfono.	
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="EmailType">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza el proposito de la dirección de correo electrónica.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="EmailAddr">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la dirección de correo electrónica.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="DeliveryDestination">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza donde entregar los cheques, tales como oficina,
						cliente y dirección alternativa.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="PostAddr">
				<xsd:annotation>
					<xsd:documentation>
						En este campo se parametriza la dirección postal.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
      <xsd:element name="ContactInfo" type="bbog:ContactInfo_Type"/>
    </xsd:schema>
